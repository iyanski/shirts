<?php

/*
Description: Converts Array to iML

Example:
$arr = array('a','b','c','d');
$a = new Artoml;
echo $a->convert($arr);

*/
class Artomi{

	
	function convert($arr)
	{
		$tmp = '';
		foreach($arr as $key => $value){
				$tmp .=  '<' . $key . '>' . $value . '</' . $key . ">\n";
			}	
		return $tmp;
	}
	
}
?>